    "@zeix/cause-effect": "0.17.3",
    "alien-signals": "^2.0.8",
    "compostate": "^0.5.1",
    "esbuild": "^0.25.12",
    "mobx": "^6.15.0",
    "mol_wire_lib": "^1.0.1644",
